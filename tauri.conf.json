{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "web-app",
  "version": "0.1.0",
  "identifier": "web-app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5000"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Web App",
        "center": true,
        "focus": true,
        "devtools": false,
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ]
  }
}
